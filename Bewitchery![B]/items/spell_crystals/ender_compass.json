{
	"format_version": "1.18.0",
	"minecraft:item": {
		"description": {
			"identifier": "bw:ender_compass",
			"category": "items"
		},
		"components": {
			"minecraft:creative_category": {
				"parent": "itemGroup.name.relics"
			},
			"minecraft:max_stack_size": 64,
			"minecraft:hand_equipped": false,
			"minecraft:icon": {
				"texture": "ender_compass"
			},
			"minecraft:cooldown": {
			  "category": "bw:compass",
			  "duration": 0.5
			},
			"minecraft:on_use": {
			  "on_use": {
			    "event": "activate_warp"
			  }
			}
		},
		"events": {
		  "activate_warp": {
		    "sequence": [
		      {
		        "condition": "q.scoreboard('bw:p_Blocked') == 0",
		        "sequence": [
		          {
		            "condition": "q.scoreboard('bw:spellSlot1') > 0 && q.scoreboard('bw:spellSlot2') == 0",
        		    "run_command": {
        		      "command": [
        		        "particle bw:ender_spell_phase_two ~~~",
        		        "playsound beacon.activate @a[r=6] ~~~ 0.8",
        		        "scoreboard players set @s bw:spellSlot2 6",
        		        "scoreboard players set @s bw:spellTime 0"
        		      ]
        		    }
		          }
		        ]
		      },
		      {
		        "condition": "q.scoreboard('bw:p_Blocked') >= 1",
		        "run_command": {
		          "command": [
		            "tellraw @s {\"rawtext\":[{\"text\":\"A strange force blocks your connection to the Primal Elements. You are unable to cast Warp spells.\"}]}",
		            "scoreboard players set @s bw:spellSlot1 0",
		            "scoreboard players set @s bw:spellSlot2 0",
		            "scoreboard players set @s bw:spellTime 0"
		          ]
		        }
		      }
		    ]
		  }
		}
	}
}