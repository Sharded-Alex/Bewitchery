{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "bw:warp_dummy",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false,
      "scripts": {
        "animate": [
          "mushroom_ring"
        ]
      },
      "animations": {
        "mushroom_ring": "controller.animation.detect_shrooms"
      }
    },
    "component_groups": {
      "break_death": {
        "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:nameable": {
        "always_show": true
      },
      "minecraft:collision_box": {
        "width": 1,
        "height": 1
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "cause": "all",
          "deals_damage": false
        }
      },
      "minecraft:interact": {
        "interactions": [
          {
            "on_interact": {
              "filters": { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "bw:amethyst_dust"},
              "event": "bw:is_warping"
            },
            "use_item": true,
            "interact_text": "action.interact.warp"
          }
        ]
      },
      "minecraft:type_family": {
        "family": [ "warp_dummy" ]
      },
      "minecraft:physics": {
        "has_gravity": true
      },
      "minecraft:scale": {
        "value": 0.25
      }
    },
    "events": {
      "death": {
        "add": {
          "component_groups": [
            "break_death"
          ]
        }
      },
      "bw:is_warping": {
        "run_command": {
          "command": [
            "function warp/warp_entities"
          ],
          "target": "self"
        }
      }
    }
  }
}