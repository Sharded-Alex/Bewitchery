{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "bw:ender_chest_bug",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false,
      "scripts": {
        "animate": [
          "despawn_items"
        ]
      },
      "animations": {
        "despawn_items": "controller.animation.summon"
      }
    },
    "component_groups": {
      "countdown": {
        "minecraft:attack_cooldown": {
          "attack_cooldown_time": [20.0, 20.0],
          "attack_cooldown_complete_event": {
            "event": "bw:disappears",
            "target": "self"
          }
        }
      },
      "shift": {
        "minecraft:skin_id": {
          "value": 1
        }
      },
      "break_death": {
        "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:collision_box": {
        "width": 0.15,
        "height": 0.15
      },
      "minecraft:type_family": {
        "family": [ "spell_dummy" ]
      },
      "minecraft:skin_id": {
        "value": 0
      },
      "minecraft:environment_sensor": {
        "triggers": [
          {
            "event": "bw:kill_block",
            "filters": {
              "all_of": [
                {
                  "test": "in_block",
                  "operator": "!=",
                  "value": "minecraft:ender_chest"
                }
              ]
            }
          }
        ]
      },
      "minecraft:block_sensor": {
        "sensor_radius": 1,
        "sources": [
          {
            "test": "in_block",
            "operator": "!=",
            "value": "minecraft:ender_chest"
          }
        ],
        "on_break": [
          {
            "block_list": [
              "minecraft:ender_chest"
            ],
            "on_block_broken": {
              "event": "bw:kill_block",
              "target": "self"
            }
          }
        ]
      },
      "minecraft:physics": {},
      "minecraft:scale": {
        "value": 0.25
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "countdown"
          ]
        }
      },
      "bw:kill_block": {
        "remove": {
          "component_groups": [
            "countdown"
          ]
        },
        "add": {
          "component_groups": [
            "shift"
          ]
        }
      },
      "bw:disappears": {
        "remove": {
          "component_groups": [
            "countdown"
          ]
        },
        "run_command": {
          "command": [
            "setblock ~~~ minecraft:air 0 replace"
          ]
        },
        "add": {
          "component_groups": [
            "break_death"
          ]
        }
      }
    }
  }
}