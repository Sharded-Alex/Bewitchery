{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.trails": {
      "states": {
        "default": {
          "transitions": [ { "walking": "(0.1)" } ],
          "on_entry": [ "/function b_particles" ]
        },
        "walking": {
          "transitions": [ { "default": "(0.1)" } ],
          "on_entry": [ "/function b_particles" ]
        }
      }
    },
    "controller.animation.bursts": {
      "states": {
        "default": {
          "transitions": [
            {
              "blaze_fire_burst": "q.skin_id == 1"
            },
            {
              "despawn": "q.skin_id == 2"
            }
          ]
        },
        "blaze_fire_burst": {
          "on_entry": [
            "/particle bw:blaze_fire_burst ~~~",
            "/fill ~-1~~-1~1~1~1 candle  [\"lit\": true] replace candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 black_candle  [\"lit\": true] replace black_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 blue_candle  [\"lit\": true] replace blue_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 cyan_candle  [\"lit\": true] replace cyan_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 gray_candle  [\"lit\": true] replace gray_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 green_candle  [\"lit\": true] replace green_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 light_blue_candle  [\"lit\": true] replace light_blue_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 light_gray_candle  [\"lit\": true] replace light_gray_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 lime_candle  [\"lit\": true] replace lime_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 magenta_candle  [\"lit\": true] replace magenta_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 orange_candle  [\"lit\": true] replace orange_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 pink_candle  [\"lit\": true] replace pink_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 purple_candle  [\"lit\": true] replace purple_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 red_candle  [\"lit\": true] replace red_candle  [\"lit\": false]",
            "/fill ~-1~~-1~1~1~1 white_candle  [\"lit\": true] replace white_candle  [\"lit\": false]",
            "@s death"
          ]
        },
        "despawn": {
          "on_entry": [
            "@s death"
          ]
        }
      }
    },
    "controller.animation.curses": {
      "states": {
        "default": {
          "transitions": [
            {
              "fading_light": "q.skin_id == 2"
            },
            {
              "halted_vitality": "q.skin_id == 3"
            },
            {
              "lunar_binding": "q.skin_id == 4"
            },
            {
              "talisman_binding": "q.skin_id == 5"
            },
            {
              "primal_binding": "q.skin_id == 6"
            },
            {
              "hydrophobia": "q.skin_id == 7"
            },
            {
              "psychosis": "q.skin_id == 8"
            },
            {
              "famine": "q.skin_id == 9"
            }
          ]
        },
        "fading_light": {
          "on_entry": [
            "/scoreboard players set @e[r=2] bw:darkCurse_D 72000",
            "/scoreboard players random @e[r=2] bw:darkRand 1 4",
            "@s death"
          ]
        },
        "halted_vitality": {
          "on_entry": [
            "/scoreboard players set @e[r=2] bw:regenCurse 72000",
            "@s death"
          ]
        },
        "lunar_binding": {
          "on_entry": [
            "/scoreboard players set @a[r=2] bw:lBind_Curse 72000",
            "@s death"
          ]
        },
        "talisman_binding": {
          "on_entry": [
            "/scoreboard players set @a[r=2] bw:tBlocked 72000",
            "@s death"
          ]
        },
        "primal_binding": {
          "on_entry": [
            "/scoreboard players set @a[r=2] bw:p_Blocked 72000",
            "@s death"
          ]
        },
        "hydrophobia": {
          "on_entry": [
            "/scoreboard players set @e[r=2] bw:hydroCurse 72000",
            "/scoreboard players set @e[r=2] bw:hydroCurseT 1",
            "@s death"
          ]
        },
        "psychosis": {
          "on_entry": [
            "/scoreboard players set @e[r=2] bw:hauntedC 72000",
            "/scoreboard players set @e[r=2] bw:hauntedRand 1",
            "@s death"
          ]
        },
        "famine": {
          "on_entry": [
            "/scoreboard players set @e[r=2] bw:eatCurse 72000",
            "/scoreboard players set @e[r=2] bw:eatRandom 1",
            "@s death"
          ]
        }
      }
    }
  }
}